---
title: Home
active: Home
---

<!DOCTYPE html>

<html>
  <head>
    <title>Waycrate | {{ title }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ '/css/styles.css' | url }}">
    <script src="{{ '/js/index.js'  | url }}" defer></script>
    <script src="https://kit.fontawesome.com/86c060d6eb.js" crossorigin="anonymous"></script>
    <link rel="icon" type="image/x-icon" href="{{ '/assets/img/favicon.ico' | url }}">
  </head>
  <body>
    <div id="container">
      <header>
        <div class="logo">
          <img src="{{ '/assets/img/waycrate-logo.png' | url }}" alt="Waycrate Logo">
        </div>

        <h1>Waycrate</h1>

        <nav>
          <ul>
            <li {% if title == "Home" %}class="active"{% endif %}><a href="{{ '/index.html' | url }}"><i class="fa-solid fa-house"></i> Home</a></li>
            <li><a href="https://github.com/waycrate/"><i class="fa-brands fa-github"></i> GitHub</a></li>
            <li><a href="https://discord.gg/KKZRDYrRYW"><i class="fa-brands fa-discord"></i> Discord</a></li>
          </ul>
        </nav>

        <h2>Projects</h2>

        <nav>
          <ul>
            <li {% if title == "swhkd" %}class="active"{% endif %}><a href="{{ '/swhkd/' | url }}">swhkd</a></li>
            <ul>
                <li {% if title == "Install" %}class="active"{% endif %}><a href="{{ '/swhkd/install' | url }}">install</a></li>
            </ul>
            <li {% if title == "Wayshot" %}class="active"{% endif %}><a href="{{ '/wayshot/' | url }}">Wayshot</a></li>
            <li {% if title == "Wayout" %}class="active"{% endif %}><a href="{{ '/wayout/' | url }}">Wayout</a></li>
          </ul>
        </nav>

        <button class="dark-mode-toggle"><i class="fa-solid fa-toggle-off"></i><i class="fa-solid fa-toggle-on"></i></button>
      </header>

      <div class="content">
        {{ content | safe }}
      </div>
    </div>
  </body>
</html>
